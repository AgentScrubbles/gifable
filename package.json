{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prebuild": "npm run install:css",
    "prestart:test-server": "DATABASE_URL=file:./test.db npm run db:push",
    "start:test-server": "ENV_FILE=.env.test remix dev",
    "install:css": "curl -sL https://cdn.simplecss.org/simple.min.css -o public/css/simple.min.css",
    "build": "remix build",
    "dev": "DEBUG=app:* remix dev",
    "predev": "npm run install:css",
    "prestart": "node ./seed-admin.js",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve build",
    "typecheck": "tsc",
    "test": "jest app",
    "test:watch": "jest app --watch",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "node --require esbuild-register app/db/seed.ts",
    "build:seed-admin": "esbuild seed-admin.ts --bundle --platform=node --format=cjs --outfile=seed-admin.js --external:postgres --external:better-sqlite3 --external:drizzle-orm --external:bcryptjs --external:dotenv",
    "migrate:sqlite-to-postgres": "node scripts/migrate-sqlite-to-postgres.js"
  },
  "dependencies": {
    "@remix-run/node": "^1.16.1",
    "@remix-run/react": "^1.16.1",
    "@remix-run/serve": "^1.16.1",
    "@remix-validated-form/with-zod": "^2.0.5",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^9.2.2",
    "bytes": "^3.1.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.29.3",
    "fetch-h2": "^3.0.2",
    "hasha": "^5.2.2",
    "isbot": "^3.6.5",
    "jimp": "^0.22.7",
    "lru-cache": "^9.1.1",
    "minio": "^7.1.1",
    "ms": "^2.1.3",
    "postgres": "^3.4.3",
    "quantize": "^1.0.2",
    "rate-limiter-flexible": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-utils": "^6.5.0",
    "remix-validated-form": "^4.6.11",
    "zod": "^3.21.4",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.34.1",
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/better-sqlite3": "^7.6.9",
    "@types/bytes": "^3.1.1",
    "@types/jest": "^29.5.0",
    "@types/minio": "^7.1.0",
    "@types/ms": "^0.7.31",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "drizzle-kit": "^0.20.9",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.27.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
}
