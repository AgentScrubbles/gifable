  "license": "MIT",
  "scripts": {
    "dev": "npx nodemon",
    "start": "yarn build && node build/main.js",
    "build": "rm -rf ./build && tsc",
    "fmt": "prettier --write .",
    "pretest": "rm -f ./prisma/test.db && DATABASE_URL=file:./test.db prisma migrate deploy",
    "test": "jest src",
    "predev": "prisma generate",
    "migrate": "prisma migrate deploy",
    "start:docker": "yarn migrate && node build/main.js",
    "build:docker": "docker build -t pietvanzoen/gifme:latest ."
  },
  "dependencies": {
    "@fastify/secure-session": "^6.0.0",
