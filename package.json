    "@fastify/view": "^7.4.1",
    "@prisma/client": "^4.12.0",
    "@sinclair/typebox": "^0.26.8",
    "bytes": "^3.1.2",
    "colorthief": "^2.4.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "eta": "^2.0.1",
    "fastify": "^4.15.0",
    "fetch-h2": "^3.0.2",
    "http-errors": "^2.0.0",
    "jimp": "^0.22.7",
    "minio": "^7.0.33",
    "ms": "^2.1.3",
    "pino-pretty": "^10.0.0",
    "prisma": "^4.12.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/chance": "^1.1.3",
    "@types/debug": "^4.1.7",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.0",
    "@types/minio": "^7.0.18",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.15.10",
    "chance": "^1.1.11",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  }
